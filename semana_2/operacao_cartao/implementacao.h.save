#ifndef IMPLEMENTACAO_H_INCLUDED
#define IMPLEMENTACAO_H_INCLUDED

#define STR_LEN 255


struct data{
    int dia;
    int mes;
    int ano;
};

typedef struct data Data;

struct operacao{
    char estabelecimento[STR_LEN];
    float valor;
    Data data
};

typedef struct operacao Operacao;

Operacao ler_operacao();

Operacao maior_operacao(Operacao[], int );

float soma_operacoes(Operacao[], int );


#endif // IMPLEMENTACAO_H_INCLUDED
